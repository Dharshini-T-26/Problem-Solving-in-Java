import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int sum =0;
        int a =(int) Math.pow(n,2);
        
        while(a>0)
        {
            int b = a % 10;
            sum = sum + b;
            a /= 10;
        }
    
        if(sum == n)
            System.out.println("true");
        else
            System.out.println("false");
        
    }
}
