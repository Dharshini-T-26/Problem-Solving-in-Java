import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int sum = 0;
        int res = 1;
        
        for(int i =1;i<=n;i++)
        {
            sum += i;
            res = res*i;
        }
        System.out.println(sum);
        
        System.out.println(res);
        
        s.close();
        

    }
}
