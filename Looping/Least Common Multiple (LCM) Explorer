import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        
        int x = a, y = b;
        
        // GCD using Euclidean algorithm (loop-based)
        while (y != 0) {
            int temp = y;
            y = x % y;
            x = temp;
        }
        
        int gcd = x;
        int lcm = (a * b) / gcd;
        
        System.out.println(lcm);
    }
}
