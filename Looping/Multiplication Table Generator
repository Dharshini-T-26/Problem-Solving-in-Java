import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        
        for(int i=1;i<=10;i++)
        {
            int result = n *i;
            System.out.printf("%d * %d = %d",n,i,result);
            System.out.println();
        }
    }
}
